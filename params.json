  "body": "<!doctype html> <!-- html5 -->\r\n<html>\r\n    <head>\r\n        <title>Dots And Boxes</title>\r\n        <meta name=\"author\" content=\"azthec\" />\r\n        <meta name=\"viewport\" content=\"width=device-width\">\r\n        <link rel=\"stylesheet\" href=\"DnB_part_2/css/dnb.css\" type=\"text/css\" />\r\n        <link rel=\"icon\" type=\"image/png\" href=\"DnB_part_2/img/browser_icon.png\" sizes=\"48x48\">\r\n    </head>\r\n    <body>\r\n        <script src=\"DnB_part_2/js/divmain.js\"></script>\r\n        <script src=\"DnB_part_2/js/divtabs.js\"></script>  \r\n        <script src=\"DnB_part_2/js/login.js\"></script>\r\n        <script src=\"DnB_part_2/js/divgame.js\"></script>\r\n        <script src=\"DnB_part_2/js/divtable.js\"></script>\r\n        <script src=\"DnB_part_2/js/onlinegame.js\"></script>\r\n        <script src=\"DnB_part_2/js/closures.js\"></script>\r\n\r\n        <!-- GAME OPTIONS AND STATS DIV -->\r\n        <div id=\"MainDiv\" style=\"display: block\">\r\n            <ul class=\"tab\">\r\n                <li><a href=\"javascript:void(0)\" class=\"tablinks\" onclick=\"openTab(event, 'Game')\">Game</a></li>\r\n                <li><a href=\"javascript:void(0)\" class=\"tablinks\" onclick=\"openTab(event, 'Options')\">Options</a></li>\r\n                <li><a href=\"javascript:void(0)\" class=\"tablinks\" onclick=\"openTab(event, 'Leaderboards')\">Leaderboards</a></li>\r\n            </ul>\r\n\r\n            <div id=\"Game\" class=\"tabcontent\">\r\n                <a href=\"javascript:void(0)\" id=startButton class=\"button\" onclick=\"startGame(); toggleButton('startButton');\">New Game</a>\r\n\r\n                <div id=\"login\" class=\"container\" style=\"display:none;\">\r\n                    <label><b>Username</b></label>\r\n                    <!-- Username 5-8 characters -->\r\n                    <input type=\"text\" placeholder=\"Enter Username\" id=\"uname\" name=\"uname\" pattern=\"([A-Za-z]){5,8}\" required>\r\n                    <label><b>Password</b></label>\r\n                    <!-- Password 8characters+ lowercase uppercase symbol -->\r\n                    <input type=\"password\" placeholder=\"Enter Password\" id=\"psw\" name=\"psw\" pattern=\"(?=^.{8,}$)((?=.*\\d)|(?=.*\\W+))(?![.\\n])(?=.*[A-Z])(?=.*[a-z]).*$\" required>\r\n\r\n                    <button id=\"submitButton\" type=\"submit\" onclick=\"check()\">Login</button>\r\n                </div>\r\n\r\n                <div id=\"GameArea\" style=\"display:none;\">\r\n\r\n                </div>\r\n\r\n                <div id=\"GameProgress\" style=\"display:none;\">\r\n\r\n                    <!-- GAME STATS GOES HERE -->\r\n                    <div id=\"disTimerPlayer\">\r\n                    </div>\r\n                    <div id=\"disTimerCPU\">\r\n                    </div>\r\n\r\n                    <p>Player Score</p>\r\n                    <div id=\"playerscore\"></div>\r\n                    <p>Cpu Score</p>\r\n                    <div id=\"cpuscore\"></div>\r\n\r\n                    <div id=\"victorystatus\"></div>\r\n\r\n                    <a href=\"javascript:void(0)\" class=\"button\" onclick=\"removeTable(); toggleDiv('MainDiv');  toggleDiv('WelcomeDiv')\">Quit</a>\r\n                </div> \r\n            </div>\r\n\r\n            <div id=\"Options\" class=\"tabcontent\">\r\n                <h1>Options</h1>\r\n                <form id=\"difficulty\">\r\n                    <input type=\"checkbox\" id=\"onlineCheckbox\" name=\"mode\" value=\"online\" checked>Online<br>\r\n                    <br>\r\n                    <div id=\"radioDiv\">\r\n                        Select difficulty:<br>\r\n                        <input type=\"radio\" name=\"difficulty\" class=\"radio\" value=\"beg\" checked> Beginner (2 by 3)<br>\r\n                        <input type=\"radio\" name=\"difficulty\" class=\"radio\" value=\"inter\"> Intermediate (4 by 5)<br>\r\n                        <input type=\"radio\" name=\"difficulty\" class=\"radio\" value=\"adv\"> Advanced (6 by 8)<br>\r\n                        <input type=\"radio\" name=\"difficulty\" class=\"radio\" value=\"exp\"> Expert (9 by 11)<br>\r\n                    </div> \r\n                </form>\r\n            </div>\r\n\r\n            <div id=\"Leaderboards\" class=\"tabcontent\">\r\n                <h1>Leaderboards</h1>\r\n                <form id=\"rankingDifficulty\">\r\n                    <input type=\"checkbox\" id=\"rankingOnlineCheckbox\" name=\"mode\" value=\"online\" checked>Online<br>\r\n                    <br>\r\n                    <div id=\"radioDiv\">\r\n                        Select difficulty:<br>\r\n                        <input type=\"radio\" name=\"rankingDifficulty\" class=\"radio\" value=\"beg\" checked> Beginner (2 by 3)<br>\r\n                        <input type=\"radio\" name=\"rankingDifficulty\" class=\"radio\" value=\"inter\"> Intermediate (4 by 5)<br>\r\n                        <input type=\"radio\" name=\"rankingDifficulty\" class=\"radio\" value=\"adv\"> Advanced (6 by 8)<br>\r\n                        <input type=\"radio\" name=\"rankingDifficulty\" class=\"radio\" value=\"exp\"> Expert (9 by 11)<br>\r\n                    </div> \r\n                </form>\r\n                <br>\r\n                <input type=\"button\" class=\"button\" id=\"tablebutton\" value=\"Get Leaderboards\" onclick=\"rankingTable();\">\r\n                <br>\r\n                <div id=\"leaderboard_table\">\r\n                    \r\n                </div>\r\n            </div>\r\n\r\n\r\n        </div>\r\n    </body>\r\n</html>",
  "note": "Don't delete this file! It's used internally to help with page regeneration."
}